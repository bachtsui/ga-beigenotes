<h2> Pending Notes </h2>
  <% @notes.each do |note| %>
  	<% if note.pending == true%>
	    <ul>
	      <li>
	        <a href= "notes/<%=note.id%>">
<!-- 	        Sent To: <%=note.users.first.contacts.last.name%>
	        <br> -->
	        Created: <%=note.created_at.strftime('%B %d, %Y')%> </a>
	      </li>
	    </ul>
		<% end %>
	<% end %> 