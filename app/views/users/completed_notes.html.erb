<div class="container-fluid">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">

		<h2 class="text-center"> Completed Notes </h2>
		  <% @notes.each do |note| %>
		  	<% if note.completed == true%>
					<div class="panel panel-default">
					  <div class="panel-body">
						  <ul>
						    <li>
						      <a href= "notes/<%=note.id%>">
						      From: <%=note.users.last.first_name%>
						      <br>
						      Created: <%=note.created_at.strftime('%B %d, %Y')%> </a>
						    </li>
						  </ul>
					  </div>
					</div>
				<% end %>
			<% end %> 

		</div>
	</div>
</div>

